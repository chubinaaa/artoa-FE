{
    "name": "artoa-front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier .",
        "format:write": "prettier . --write",
        "prepare": "husky"
    },
    "dependencies": {
        "next": "15.0.0",
        "react": "19.0.0-rc-65a56d0e-20241020",
        "react-dom": "19.0.0-rc-65a56d0e-20241020"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "npm:types-react@19.0.0-rc.1",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
        "eslint": "^8",
        "eslint-config-next": "15.0.0",
        "husky": "^9.1.6",
        "lint-staged": "^15.2.10",
        "postcss": "^8",
        "prettier": "3.3.3",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "*.css": "prettier --write",
        "*.json": "prettier --write"
    },
    "overrides": {
        "@types/react": "npm:types-react@19.0.0-rc.1",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
}
